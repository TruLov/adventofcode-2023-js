{
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "env": {
    "es2022": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "prefer-const": "warn",
    "no-unused-vars": "warn",
    "semi": "error"
  }
}
